    client_header_timeout 10s;
    client_body_timeout 10s;

    server {
        listen 8080;
        server_name localhost;

        root /usr/share/nginx/html;

        limit_req zone=mylimit burst=70 nodelay;
        limit_conn limitperip 10;

        if ($request_method !~ ^(GET|HEAD|POST)$) {
            return 444;
